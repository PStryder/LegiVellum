# Problemata demo environment (copy to .env)

# CogniGate requires an AI API key (set to a real provider key).
COGNIGATE_AI_API_KEY=replace-me

# Optional CogniGate overrides
COGNIGATE_AI_ENDPOINT=https://openrouter.ai/api/v1
COGNIGATE_AI_MODEL=anthropic/claude-3-opus
COGNIGATE_WORKER_ID=cognigate-demo-1

# Optional AsyncGate auth key (used by CogniGate when polling)
ASYNCGATE_API_KEY=dev_asyncgate_key

# MetaGate seed defaults
PROBLEMATA_TENANT_KEY=default
PROBLEMATA_DEPLOYMENT_KEY=local
PROBLEMATA_PRINCIPAL_KEY=problemata-demo-principal
PROBLEMATA_AUTH_SUBJECT=problemata-demo-subject
PROBLEMATA_PROFILE_KEY=problemata-demo-profile
PROBLEMATA_MANIFEST_KEY=problemata-demo-manifest

# MetaGate JWT secret (dev)
METAGATE_JWT_SECRET=dev-secret
